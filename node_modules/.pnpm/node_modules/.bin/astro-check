#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rafael-momo/Documents/website/astro-portfolio/node_modules/.pnpm/@astrojs+check@0.5.10_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/check/dist/node_modules:/home/rafael-momo/Documents/website/astro-portfolio/node_modules/.pnpm/@astrojs+check@0.5.10_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/check/node_modules:/home/rafael-momo/Documents/website/astro-portfolio/node_modules/.pnpm/@astrojs+check@0.5.10_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/node_modules:/home/rafael-momo/Documents/website/astro-portfolio/node_modules/.pnpm/@astrojs+check@0.5.10_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules:/home/rafael-momo/Documents/website/astro-portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rafael-momo/Documents/website/astro-portfolio/node_modules/.pnpm/@astrojs+check@0.5.10_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/check/dist/node_modules:/home/rafael-momo/Documents/website/astro-portfolio/node_modules/.pnpm/@astrojs+check@0.5.10_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/check/node_modules:/home/rafael-momo/Documents/website/astro-portfolio/node_modules/.pnpm/@astrojs+check@0.5.10_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules/@astrojs/node_modules:/home/rafael-momo/Documents/website/astro-portfolio/node_modules/.pnpm/@astrojs+check@0.5.10_prettier-plugin-astro@0.13.0_prettier@3.6.2_typescript@5.9.3/node_modules:/home/rafael-momo/Documents/website/astro-portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@astrojs/check/dist/bin.js" "$@"
else
  exec node  "$basedir/../@astrojs/check/dist/bin.js" "$@"
fi
